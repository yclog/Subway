<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/Whitecolorbackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/titlebar_home"></include>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.self.ylog.subway.View.StickyLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <android.support.v4.view.ViewPager
                android:id="@+id/Home_Ad_ViewPager"
                android:layout_width="match_parent"
                android:layout_height="140dp">
            </android.support.v4.view.ViewPager>

            <include layout="@layout/subheader_home"></include>

            <com.self.ylog.subway.View.TestRecylerView
                android:id="@+id/Home_RecyclerView"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            </com.self.ylog.subway.View.TestRecylerView>
        </com.self.ylog.subway.View.StickyLayout>
    </android.support.v4.widget.NestedScrollView>

</LinearLayout>